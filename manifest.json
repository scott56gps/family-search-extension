{
    "manifest_version": 2,

    "name": "Family History Collaborator",
    "description": "This extension helps a family member share temple work with their family",
    "version": "1.0",

    "browser_action": {
        "default_icon": "fan-chart-icon.png",
        "default_popup": "popup.html"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "content_scripts": [
        {
            "matches": ["https://familysearch.org/tree/person/*"],
            "js": ["popup.js"]
        }
    ],
    "background": {
        //"page": "background_page.html",
        "script": ["popup.js"]
    },
    "permissions": [
    "tabs",
    "https://familysearch.org/tree/person/*"
  ],
    "web_accessible_resources": ["redirect.html", "redirect.js", "popup.html"]
}
